        <activity android:name=".CategoryActivity"></activity>
        <activity android:name=".PersonalActivity"></activity>
        <activity android:name=".AlterUsrInfoActivity"></activity>
        <activity android:name=".AlterUsrNeedActivity"></activity>
    </application>

</manifest>
