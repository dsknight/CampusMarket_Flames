        </activity>
        <activity android:name=".SignupActivity" android:label="@string/button_signup"></activity>
        <activity android:name=".GoodsListActivity"></activity>
        <activity android:name=".MainPageActivity"></activity>
        <activity android:name=".GoodsActivity"></activity>
        <activity android:name=".HomeActivity"></activity>
